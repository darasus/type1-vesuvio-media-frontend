{
  "name": "utb-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd next dev",
    "build": "yarn generate:types && yarn generate:sitemap && next build",
    "preexport": "yarn build",
    "export": "next export",
    "prestart": "yarn export",
    "start": "serve out",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "deploy:all": "ts-node --project tsconfig.scripts.json scripts/deploy-all-sites.ts",
    "generate:types": "cross-env ts-node --project tsconfig.scripts.json scripts/generate-api-types.ts",
    "generate:sitemap": "cross-env ts-node --project tsconfig.scripts.json scripts/generate-sitemap.ts"
  },
  "dependencies": {
    "@types/prettier": "^2.0.2",
    "@vercel/client": "^8.1.0",
    "clsx": "^1.1.1",
    "cors": "^2.8.5",
    "next": "^9.5.1",
    "next-compose-plugins": "^2.2.0",
    "pino": "^6.5.0",
    "pino-logflare": "^0.3.5",
    "polished": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-markdown": "^4.3.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@next/bundle-analyzer": "^9.4.4",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/node": "^14.0.9",
    "@types/react": "^16.9.35",
    "@vercel/build-utils": "^2.4.0",
    "autoprefixer": "^9.8.0",
    "cross-env": "^7.0.2",
    "env-cmd": "^10.1.0",
    "globby": "^11.0.1",
    "isomorphic-fetch": "^2.2.1",
    "json-ts": "^1.6.4",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "serve": "^11.3.2",
    "tailwindcss": "^1.4.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
